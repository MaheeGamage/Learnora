{
  "python.defaultInterpreterPath": "${workspaceFolder}/core-service/.venv/Scripts/python.exe",
  "python.analysis.extraPaths": [
    "${workspaceFolder}/core-service",
    "${workspaceFolder}/core-service/app"
  ],
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticMode": "workspace",
  "[python]": {
    "editor.formatOnSave": false
  },
  "python.terminal.activateEnvironment": true,
  "python.envFile": "${workspaceFolder}/core-service/.env"
}
