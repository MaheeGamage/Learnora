# Minimal .env for Docker Compose local testing
# Copy this file to .env and update with your actual values

# Required
APP_ENV=development
DEBUG=True
SECRET_KEY=your-secret-key-here-change-in-production
DATABASE_URL=sqlite:///./learnora.db
DB_ECHO=False
GOOGLE_API_KEY=your_google_api_key_here

# Optional - Leave empty or remove if not using LangSmith
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=
LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com

# Optional - Leave commented out if not needed
# DB_POOL_SIZE=3
# DB_MAX_OVERFLOW=0
